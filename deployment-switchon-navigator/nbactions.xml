<?xml version="1.0" encoding="UTF-8"?>
<actions>
    <action>
        <actionName>build</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>install</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>clean</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>clean</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>test</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>test</goal>
        </goals>
         <properties>
            <mdep.skip>false</mdep.skip>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild-test</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>test.single</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>test-compile</goal>
            <goal>surefire:test</goal>
        </goals>
        <properties>
            <test>${packageClassName}</test>
            <mdep.skip>false</mdep.skip>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild-test</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>run.single.main</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <exec.args>-classpath %classpath ${packageClassName}</exec.args>
            <exec.executable>java</exec.executable>
            <exec.classpathScope>${classPathScope}</exec.classpathScope>
            <mdep.skip>true</mdep.skip>
            <cids.generate-lib.skip>true</cids.generate-lib.skip>
            <exec.skip>false</exec.skip>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>debug.single.main</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -classpath %classpath ${packageClassName}</exec.args>
            <exec.executable>java</exec.executable>
            <exec.classpathScope>${classPathScope}</exec.classpathScope>
            <jpda.listen>true</jpda.listen>
            <mdep.skip>true</mdep.skip>
            <cids.generate-lib.skip>true</cids.generate-lib.skip>
            <exec.skip>false</exec.skip>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>debug.test.single</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>test-compile</goal>
            <goal>surefire:test</goal>
        </goals>
        <properties>
            <test>${packageClassName}</test>
            <forkMode>once</forkMode>
            <maven.surefire.debug>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address}</maven.surefire.debug>
            <jpda.listen>true</jpda.listen>
            <mdep.skip>false</mdep.skip>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild-test</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>profile.single.main</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <exec.args>-classpath %classpath ${packageClassName}</exec.args>
            <exec.executable>java</exec.executable>
            <exec.classpathScope>${classPathScope}</exec.classpathScope>
            <mdep.skip>true</mdep.skip>
            <cids.generate-lib.skip>true</cids.generate-lib.skip>
            <exec.skip>false</exec.skip>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>javadoc</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>generate-sources</goal>
            <goal>javadoc:javadoc</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>CUSTOM-run</actionName>
        <displayName>run</displayName>
        <packagings>
            <packaging>jar</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <working.dir>${de.cismet.cids.navigator.workingDir.switchon}</working.dir>
            <exec.workingdir>${working.dir}</exec.workingdir>
            <exec.classpathScope>runtime</exec.classpathScope>
            <exec.args>-Duser.country=DE -Duser.language=de -Ddirectory.extension=WunDa.netbeans -Djdk.tls.client.protocols=TLSv1 -Xms256m -Xmx2g -classpath ${de.cismet.cids.classpath} Sirius.navigator.Navigator ${working.dir}/config/log4j.debug.properties ${working.dir}/config/navigator.cfg ${working.dir} ${working.dir}/plugins ${working.dir}/search ${working.dir}/profiles</exec.args>
            <exec.executable>java</exec.executable>
            <mdep.skip>true</mdep.skip>
            <cids.generate-lib.skip>true</cids.generate-lib.skip>
            <exec.skip>false</exec.skip>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
            <activatedProfile>cismet-dev-exec</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>CUSTOM-debug</actionName>
        <displayName>debug</displayName>
        <packagings>
            <packaging>jar</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <working.dir>${de.cismet.cids.navigator.workingDir.switchon}</working.dir>
            <exec.workingdir>${working.dir}</exec.workingdir>
            <exec.classpathScope>runtime</exec.classpathScope>
            <exec.args>-Duser.country=DE -Duser.language=de -Ddirectory.extension=WunDa.netbeans -Djdk.tls.client.protocols=TLSv1 -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -Xms256m -Xmx2g -classpath ${de.cismet.cids.classpath} Sirius.navigator.Navigator ${working.dir}/config/log4j.debug.properties ${working.dir}/config/navigator.cfg ${working.dir} ${working.dir}/plugins ${working.dir}/search ${working.dir}/profiles</exec.args>
            <exec.executable>java</exec.executable>
            <jpda.listen>true</jpda.listen>
            <mdep.skip>true</mdep.skip>
            <cids.generate-lib.skip>true</cids.generate-lib.skip>
            <exec.skip>false</exec.skip>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
            <activatedProfile>cismet-dev-exec</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>CUSTOM-profile</actionName>
        <displayName>profile</displayName>
        <packagings>
            <packaging>jar</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <working.dir>${de.cismet.cids.navigator.workingDir.switchon}</working.dir>
            <exec.workingdir>${working.dir}</exec.workingdir>
            <exec.classpathScope>runtime</exec.classpathScope>
            <exec.args>${profiler.args} -Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -Duser.country=DE -Duser.language=de -Ddirectory.extension=WunDa.netbeans -Djdk.tls.client.protocols=TLSv1 -Xms256m -Xmx2g -classpath ${de.cismet.cids.classpath} Sirius.navigator.Navigator ${working.dir}/config/log4j.debug.properties ${working.dir}/config/navigator.cfg ${working.dir} ${working.dir}/plugins ${working.dir}/search ${working.dir}/profiles</exec.args>
            <exec.executable>${profiler.java}</exec.executable>
            <mdep.skip>true</mdep.skip>
            <cids.generate-lib.skip>true</cids.generate-lib.skip>
            <exec.skip>false</exec.skip>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
            <activatedProfile>cismet-dev-exec</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>CUSTOM-debug.test</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>test</goal>
        </goals>
        <properties>
            <forkMode>once</forkMode>
            <maven.surefire.debug>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address}</maven.surefire.debug>
            <jpda.listen>true</jpda.listen>
            <mdep.skip>false</mdep.skip>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild-test</activatedProfile>
        </activatedProfiles>
    </action>
</actions>